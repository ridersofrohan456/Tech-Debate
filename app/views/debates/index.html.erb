<h1>Debates#index</h1>
<p>Find me in app/views/debates/index.html.erb</p>
<ul class="list-group">
     <% @debates.each do |debate|%>
    <li class="list-group-item">
      <span class="badge"><%= debate.users.count %></span>
        <%= link_to "GO", debate %>
      <%= debate.title %>
      </li>
   <% end  %>
</ul>
